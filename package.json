{
  "name": "react-redux-koa-starter",
  "version": "1.0.0",
  "description": "Starter for a React Redux Koa application",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "scripts": {
    "webserver": "node api/src/server.js",
    "build": "node_modules/.bin/webpack",
    "dev": "DEBUG=true node_modules/.bin/webpack-dev-server",
    "start": "npm run dev",
    "clean": "rm -rf node_modules app/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alicjakujawa/react-redux-koa-starter.git"
  },
  "keywords": [
    "starter",
    "koa",
    "redux",
    "react"
  ],
  "author": "Alicja Kujawa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alicjakujawa/react-redux-koa-starter/issues"
  },
  "homepage": "https://github.com/alicjakujawa/react-redux-koa-starter#readme",
  "dependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bookshelf": "^0.10.2",
    "html-webpack-plugin": "^2.24.0",
    "knex": "^0.12.6",
    "koa": "^1.2.4",
    "koa-cors": "0.0.16",
    "koa-route": "^2.4.2",
    "pg": "^6.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "webpack-dev-server": "^1.16.2"
  }
}
